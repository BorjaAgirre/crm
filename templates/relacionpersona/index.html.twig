{% extends 'base.html.twig' %}

{% block title %}Relacionpersona index{% endblock %}

{% block body %}
    <h1>Relacionpersona index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for relacionpersona in relacionpersonas %}
            <tr>
                <td>{{ relacionpersona }}</td>
                <td>
                    <a href="{{ path('app_relacionpersona_show', {'id': relacionpersona.id}) }}">show</a>
                    <a href="{{ path('app_relacionpersona_edit', {'id': relacionpersona.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_relacionpersona_new') }}">Create new</a>
{% endblock %}
